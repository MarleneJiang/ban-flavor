{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Flutter Run (macOS)",
			"type": "shell",
			"command": "export PATH=\"/Users/marlene/code/flutter/bin:$PATH\" && flutter run -d macos",
			"group": "build",
			"isBackground": true,
			"problemMatcher": [
				"$dart-build_runner"
			],
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared"
			}
		},
		{
			"label": "Flutter Build (macOS)",
			"type": "shell",
			"command": "export PATH=\"/Users/marlene/code/flutter/bin:$PATH\" && flutter build macos",
			"group": "build",
			"problemMatcher": [
				"$dart-build_runner"
			]
		},
		{
			"label": "Flutter Test",
			"type": "shell",
			"command": "export PATH=\"/Users/marlene/code/flutter/bin:$PATH\" && flutter test",
			"group": "test",
			"problemMatcher": [
				"$dart-build_runner"
			]
		},
		{
			"label": "Flutter Clean",
			"type": "shell",
			"command": "export PATH=\"/Users/marlene/code/flutter/bin:$PATH\" && flutter clean",
			"group": "build"
		}
	]
